{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "anhskohbo/no-captcha": "^3.5",
        "easypost/easypost-php": "^6.0",
        "laravel/framework": "^10.0",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "sentry/sentry-laravel": "^3.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^7.0",
        "php-coveralls/php-coveralls": "^2.5",
        "phpunit/phpunit": "^10.0",
        "rregeer/phpunit-coverage-check": "^0.3.1",
        "spatie/laravel-ignition": "^2.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "bin-dir": "bin",
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "coverage": "XDEBUG_MODE=coverage ./bin/phpunit --coverage-html clover.html --coverage-clover build/logs/clover.xml && bin/coverage-check build/logs/clover.xml 5 --only-percentage",
        "fix": "./bin/phpcbf --standard=./node_modules/justintime50-styles/src/php/phpcs.xml .",
        "lint": "./bin/phpcs --standard=./node_modules/justintime50-styles/src/php/phpcs.xml .",
        "migrate": "docker exec -t easypost-tools-ui-easypost-tools-ui-1 php artisan migrate",
        "migrate-fresh": "docker exec -t easypost-tools-ui-easypost-tools-ui-1 php artisan migrate:fresh",
        "migrate-seed": "docker exec -t easypost-tools-ui-easypost-tools-ui-1 php artisan migrate:fresh --seed",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "seed": "docker exec -t easypost-tools-ui-easypost-tools-ui-1 php artisan db:seed",
        "test": "./bin/phpunit"
    }
}
